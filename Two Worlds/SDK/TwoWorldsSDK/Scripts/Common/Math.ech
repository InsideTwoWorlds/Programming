function int MulSin(int nValue, int nAngle)
{
    return nValue * m_arrSin[nAngle % 256] / 256;
}//--------------------------------------------------------------------------------------|

function int MulCos(int nValue, int nAngle)
{
    return nValue * m_arrCos[nAngle % 256] / 256;
}//--------------------------------------------------------------------------------------|

function void Rotate(int &nX, int &nY, int nAngle)
{
    int nRotX, nRotY;
    
    nRotX = MulCos(nX, nAngle) - MulSin(nY, nAngle);
    nRotY = MulSin(nX, nAngle) + MulCos(nY, nAngle);
    
    nX = nRotX;
    nY = nRotY;
}//--------------------------------------------------------------------------------------|

